{
 "name": "unlint.github.com",
 "version": "1.2",
 "description": "lint you pull request on github",
 "manifest_version": 2,
 "background": { "scripts": ["main.js"] },

 "permissions": [
  "tabs", 
  "http://*/*"
 ],

 "browser_action": { 
  "default_title": "unlint.github.com", 
  "default_icon": "images/icon.png"
 },

 "options_page": "options.html"
}